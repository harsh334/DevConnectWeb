{
   "version": 2,
   "builds": [
      {
         "src": "angular.json",
         "use": "@vercel/static-build",
         "env": {
            "NODE_ENV": "development"
         },
         "config": {
         "distDir": "dist/dev-connect-web"
         }
      }
   ],
   "routes": [
      {
         "src": "/(.*)",
         "dest": "/index.html"
      }
   ]
}
