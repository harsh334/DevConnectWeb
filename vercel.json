{
   "version": 2,
   "builds": [
     {
       "src": "package.json",
       "use": "@vercel/static-build",
       "config": {
         "distDir": "dist/dev-connect-web/browser"
       }
     }
   ],
   "routes": [
     {
       "src": "/(.*\\.(js|css|svg|png|jpg|woff|woff2|ttf|json))",
       "dest": "/$1"
     },
     {
       "src": "/(.*)",
       "dest": "/index.html"
     }
   ]
 }
 